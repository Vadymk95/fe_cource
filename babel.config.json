{
    "presets": [
        "@babel/preset-react",
        "@babel/preset-typescript",
        "@babel/preset-env"
    ],
    "plugins": [
        [
            "i18next-extract",
            {
                "locales": ["ru", "en"],
                "keyAsDefaultValue": true,
                "outputPath": "public/locales/{{locale}}/{{ns}}.json"
            }
        ]
    ]
}
